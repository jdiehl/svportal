<h1>Add Conference</h1>

<div class="form">
<% form_for :conference, :url => {} do |f| %>
	<%= f.name 'Name:' %>
	<%= f.short_name 'Short Name:' %>
	<%= f.year 'Year:' %>
	<%= f.email 'Email:' %>
	<%= f.volunteers 'Volunteers:' %>
	<%= f.volunteer_hours 'Volunteer Hours:' %>
	<%= f.start_date 'Start Date:' %>
	<%= f.end_date 'End Date:' %>
	<%= submit_tag 'Add Conference', :class => 'button' %>
<% end %>
</div>